	Location("CoopPlayer")
	{
        CanShow3dLocation(false); // 3d coop location is not done by location system so we need to disable normal one
		Icon("map_icon_player_white");
		ShouldRotate(true);
        CanBeMarked(false);
        MapZoomLevelVisibility(1,1,1,1);
        IgnoreFogOfWar(true);

        ShowOnRadar(true);
        Location3dDistanceTreshold(50); 
        RadarVisibilityRange(50, 20000, 20000);

        ShowOnRadarEdge(true);

        PriorityModifier(15);
        CanHideByInvisibilityZone(false);
        ShowAs3dLocationInBinoculars(true);
        //ShowAs3dLocationInSurvivorSense(true);
        ShouldSkipInterpolationIfFresh(true);
        MapZoomLevelVisibility(1,1,1,0);
        CanShowIn3dWhenOverridenByPortals(true);
        
        CanBeInteriorCulled(false);
        CanBeMarked(false);
        IgnoreFogOfWar(true);

	}

